{
  "name": "marvelapps",
  "version": "0.0.2",
  "private": true,
  "description": "a set of applications generated by @bybit/marvel-cli",
  "scripts": {
    "start": "marvel start",
    "build": "cross-env BUILD_ENV=production marvel build",
    "start:no-mock": "marvel start -n",
    "start:no-lint": "cross-env LINT_BEFORE_BABEL=false marvel start",
    "build:no-lint": "cross-env BUILD_ENV=production LINT_BEFORE_BABEL=false marvel build",
    "test": "marvel test",
    "analyze": "marvel analyze",
    "generate": "marvel generate",
    "mock": "marvel mock",
    "nativerc": "marvel nativerc",
    "build:analyze": "cross-env BUILD_ENV=production marvel build --analyze-bundle --measure-speed",
    "build:analyze:no-lint": "cross-env BUILD_ENV=production LINT_BEFORE_BABEL=false marvel build --analyze-bundle --measure-speed",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "lint:style": "stylelint --fix \"src/bybitizens/**/*.less\" --syntax less",
    "show-marvel-version": "marvel version",
    "update:check": "marvel update",
    "update:force": "marvel update --force"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@bybit/marvel-cli": "^0.0.10",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-jest": "^26.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eruda": "^2.3.3",
    "eslint": "^7.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nock": "^13.0.2",
    "prettier": "^2.1.2",
    "react-app-polyfill": "^1.0.6",
    "react-hot-loader": "^4.12.21",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.6.1",
    "typescript": "^4.0.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "marvel verifyCommitMsg"
  },
  "lint-staged": {
    "src/bybitizens/**/*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ],
    "src/bybitizens/**/*.less": [
      "stylelint --syntax less"
    ],
    "src/bybitizens/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.jsx,.ts,.tsx"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
